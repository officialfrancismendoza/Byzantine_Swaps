{
    "compilerOptions": {
        "outDir": "./build",
        "allowJs": true,
        "target": "es5",
        "esModuleInterop": true,
        "module": "commonjs",
        "lib": ["es2015"]
    },
    "include": ["src/**/*", "index.js"],
    "exclude": ["node_modules", "protos", "scripts", "test", "protos-js", "build"],
    "skipLibCheck": true,        
    "resolveJsonModule": true
}